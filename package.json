{
    "name": "@marouane-mjh/faker",
    "version": "0.1.1",
    "description": "Type-safe mock data generator for TypeScript",
    "keywords": [
        "mock",
        "faker",
        "typescript",
        "fixtures",
        "testing"
    ],
    "author": "El Moujahid Marouane",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "dev": "tsc --watch",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "acorn": "^8.15.0",
        "any-promise": "^1.3.0",
        "bundle-require": "^5.1.0",
        "cac": "^6.7.14",
        "chokidar": "^4.0.3",
        "commander": "^4.1.1",
        "confbox": "^0.1.8",
        "consola": "^3.4.2",
        "debug": "^4.4.3",
        "esbuild": "^0.27.2",
        "fdir": "^6.5.0",
        "fix-dts-default-cjs-exports": "^1.0.1",
        "joycon": "^3.1.1",
        "lilconfig": "^3.1.3",
        "lines-and-columns": "^1.2.4",
        "load-tsconfig": "^0.2.5",
        "magic-string": "^0.30.21",
        "mlly": "^1.8.0",
        "ms": "^2.1.3",
        "mz": "^2.7.0",
        "object-assign": "^4.1.1",
        "pathe": "^2.0.3",
        "picocolors": "^1.1.1",
        "picomatch": "^4.0.3",
        "pirates": "^4.0.7",
        "pkg-types": "^1.3.1",
        "postcss-load-config": "^6.0.1",
        "readdirp": "^4.1.2",
        "resolve-from": "^5.0.0",
        "rollup": "^4.54.0",
        "source-map": "^0.7.6",
        "sucrase": "^3.35.1",
        "thenify": "^3.3.1",
        "thenify-all": "^1.6.0",
        "tinyexec": "^0.3.2",
        "tinyglobby": "^0.2.15",
        "tree-kill": "^1.2.2",
        "ts-interface-checker": "^0.1.13",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "ufo": "^1.6.1"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.4.6"
    }
}
